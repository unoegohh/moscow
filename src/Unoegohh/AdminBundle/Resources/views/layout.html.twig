    <title>{% block title %}Просмотр турнира Blood & Steam{% endblock %}</title>

    <meta name="description"
          content="{% block description %} {% endblock %}">
    <meta name="author" content="unoegohh">
    {#<meta name="robots" content="noindex, nofollow">#}

    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1.0">

    <!-- Icons -->
    <!-- The following icons can be replaced with your own, they are used by desktop and mobile browsers -->
    <link rel="shortcut icon" href="/admin/img/favicon.ico">
    <link rel="apple-touch-icon" href="/admin/img/icon57.png" sizes="57x57">
    <link rel="apple-touch-icon" href="/admin/img/icon72.png" sizes="72x72">
    <link rel="apple-touch-icon" href="/admin/img/icon76.png" sizes="76x76">
    <link rel="apple-touch-icon" href="/admin/img/icon114.png" sizes="114x114">
    <link rel="apple-touch-icon" href="/admin/img/icon120.png" sizes="120x120">
    <link rel="apple-touch-icon" href="/admin/img/icon144.png" sizes="144x144">
    <link rel="apple-touch-icon" href="/admin/img/icon152.png" sizes="152x152">
    <!-- END Icons -->
    <link rel="stylesheet" href="/admin/css/bootstrap.min.css">
    <link rel="stylesheet" href="/admin/css/plugins.css">
    <link rel="stylesheet" href="/admin/css/main.css">
    <link rel="stylesheet" href="/admin/css/themes.css">
    <script src="/admin/js/ckeditor/ckeditor.js"></script>
    <script src="/admin/js/vend/modernizr-2.7.1-respond-1.4.2.min.js"></script>
</head>
<body>
<div id="page-container" class="sidebar-partial sidebar-visible-lg sidebar-no-animations header-fixed-top style-alt footer-fixed">
    <div id="sidebar">
        <!-- Wrapper for scrolling functionality -->
        <div class="sidebar-scroll">
            <!-- Sidebar Content -->
            <div class="sidebar-content">
                <!-- Brand -->
                <a href="/" class="sidebar-brand">
                    <i class="gi gi-flash"></i><strong>Магазин</strong>
                </a>
                <!-- END Brand -->

                <!-- User Info -->
                <div class="sidebar-section sidebar-user clearfix">
                    <div class="sidebar-user-avatar">
                            <img src="http://dieta.ua/images/avatars/1/bd9bbf297e560f9414c80e3aca574878-bpfull.jpg" alt="avatar">

                    </div>
                    <div class="sidebar-user-name">{{ app.security.getToken().getUser() }}</div>
                    <div class="sidebar-user-links">
                        <a href="{{ path("fos_user_security_logout") }}" data-toggle="tooltip" data-placement="bottom" title="Выйти"><i
                                    class="gi gi-exit"></i></a>
                    </div>
                </div>
                <!-- END User Info -->


                <!-- Sidebar Navigation -->
                <ul class="sidebar-nav">
                    <li>
                        <a href="{{ path("unoegohh_admin_homepage") }}"><i class="gi gi-stopwatch sidebar-nav-icon"></i>Главная</a>
                    </li>
                    {#<li>#}
                        {#<a href="javascript:void(0);" class="sidebar-nav-menu"><i#}
                                    {#class="fa fa-angle-left sidebar-nav-indicator"></i><i#}
                                    {#class="gi gi-play sidebar-nav-icon"></i>Продукция</a>#}
                        {#<ul>#}
                            {#<li>#}
                                {#<a href="{{ path("unoegohh_admin_food_item") }}">Товары</a>#}
                            {#</li>#}
                            {#<li>#}
                                {#<a href="{{ path("unoegohh_admin_food_category") }}">Категории товаров</a>#}
                            {#</li>#}
                        {#</ul>#}
                    {#</li>#}
                    <li>
                        <a href="{{ path("unoegohh_admin_posts") }}"><i class="gi gi-share_alt sidebar-nav-icon"></i>Посты</a>
                    </li>
                    <li>
                        <a href="{{ path("unoegohh_admin_static_page") }}"><i class="fa fa-file-code-o sidebar-nav-icon"></i>Статические страницы</a>
                    </li>
                    <li>
                        <a href="{{ path("unoegohh_admin_menu") }}"><i class="fa fa-navicon sidebar-nav-icon"></i>Меню</a>
                    </li>
                    <li>
                        <a href="{{ path("unoegohh_admin_site_pref") }}"><i class="gi gi-vcard sidebar-nav-icon"></i>Натройки сайта</a>
                    </li>
                    {#<li>#}
                        {#<a href="{{ path("unoegohh_admin_customer_request") }}"><i class="gi gi-crown sidebar-nav-icon"></i>Заявки</a>#}
                    {#</li>#}
                    {#<li>#}
                        {#<a href="{{ path("unoegohh_admin_banners") }}"><i class="fa fa-caret-square-o-left sidebar-nav-icon"></i>Баннеры</a>#}
                    {#</li>#}
                    {#<li>#}
                        {#<a href="{{ path("unoegohh_admin_user") }}"><i class="fa fa-user sidebar-nav-icon"></i>Пользователи</a>#}
                    {#</li>#}
                </ul>
                <!-- END Sidebar Navigation -->

            </div>
            <!-- END Sidebar Content -->
        </div>
        <!-- END Wrapper for scrolling functionality -->
    </div>
    <div id="main-container">
        <header class="navbar navbar-default navbar-fixed-top">
            <ul class="nav navbar-nav-custom">
                <li>
                    <a href="javascript:void(0)" onclick="App.sidebar('toggle-sidebar');">
                        <i class="fa fa-bars fa-fw"></i>
                    </a>
                </li>
            </ul>
        </header>
        <div id="page-content">
            {% block content %}
            {% endblock %}
        </div>

        <footer class="clearfix">
            <div class="pull-right">
                Crafted with <i class="fa fa-heart text-danger"></i> by <a href="http://goo.gl/vNS3I"
                                                                           target="_blank">pixelcave</a>
            </div>
            <div class="pull-left">
                <span id="year-copy"></span> &copy; <a href="http://goo.gl/TDOSuC" target="_blank">ProUI 1.1</a>
            </div>
        </footer>
    </div>
</div>

<!-- Scroll to top link, initialized in js/app.js - scrollToTop() -->
<a href="#" id="to-top"><i class="fa fa-angle-double-up"></i></a>

